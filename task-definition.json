{
  "family": "flask-library-app",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::828131889683:role/ecs-execution-role",
  "containerDefinitions": [
    {
      "name": "flask-library-app",
      "image": "projetomesa07/flask-library-app:latest",
      "portMappings": [
        {
          "containerPort": 5000,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ]
}
